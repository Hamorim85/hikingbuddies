<%= simple_form_for @event do |f| %>
  <%= f.error_notification %>
  <%= f.input :event_mountain, label:"Enter Moutain Name" %>
  <%= f.input :event_date, label:"Enter Date" %>
  <%= f.input :location_point, label: "Location Point" %>
  <%= f.input :event_leave_time, label: "Enter Leave Time" %>
  <%= f.input :planner,  label:"Planner name" %>
  <%= f.input :description, label: "Description" %>
  <%= f.button :submit, class:"btn btn-success" %>
<% end %>
